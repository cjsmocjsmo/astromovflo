        <div class="grid grid-cols-5 gap-2 flex items-center">
            <SheHulk title="She Hulk" src={SheHulkImg} />
            <HawkeyeCard title="Hawk Eye" src={HawkeyeImg} />
            <LokiCard title="Loki" src={LokiImg} />
            <IAmGroot title="I Am Groot" src={IAmGrootImg} />
            <FalconWinterSoldierCard title="Falcon Winter Soldier" src={FalconWinterSoldierImg} />
            <MSMarvel title="Ms Marvel" src={MSMarvelImg} />
            <WandaVision title="Wanda Vision" src={WandaVisionImg} />

