import LokiImg from '../../../images/mcu/loki.webp';
import HawkeyeImg from '../../../images/mcu/Hawkeye.webp'
import IAmGrootImg from '../../../images/mcu/iamgroot.webp'
import FalconWinterSoldierImg from '../../../images/mcu/falconwintersoldier.webp';
import MSMarvelImg from '../../../images/mcu/ms_marvel.webp'
import SheHulkImg from '../../../images/mcu/shehulk.webp';
import WandaVisionImg from '../../../images/mcu/wandavision.webp';

import LokiCard from '../../components/mcu/LokiCard.astro';
